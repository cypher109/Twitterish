<% if @user.id != session[:current_user_id] %>
  <h1>Subscribe to this person!</h1>
  <% form_for([@user,@new_subscription]) do |f| %>
    <%= f.error_messages %>
    <%= f.hidden_field :publisher_id %>
    <%= f.hidden_field :subscriber_id %>
      <p>
        <%= f.submit 'Subscribe' %>
      </p>
  <% end %>
<% end %>